{
  "$schema": "https://json.schemastore.org/web-manifest-combined.json",
  "name": "My site",
  "short_name": "My site",
  "start_url": "/",
  "display": "standalone",
  "background_color": "#f8f9fa",
  "theme_color": "#e2dcc8",
  "description": "My site description",
  "lang": "en",
  "icons": [
    {
      "src": "/images/icon-vector.svg",
